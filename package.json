{
    "name": "nova",
    "version": "0.0.1",
    "description": "A Discord bot built on Discord.JS v12 with TypeScript",
    "main": "dist/index.js",
    "dependencies": {
        "discord.js": "^12.3.1",
        "onion-lasers": "^1.1.2"
    },
    "devDependencies": {
        "@types/node": "^14.11.2",
        "@types/ws": "^7.2.6",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "eslint": "^7.26.0",
        "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
        "eslint-plugin-node": "^11.1.0",
        "husky": "^6.0.0",
        "prettier": "^2.2.1",
        "ts-node": "^9.0.0",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.0.2"
    },
    "scripts": {
        "clean": "rm -r dist",
        "build": "tsc",
        "dev": "tsc-watch --onSuccess \"node . dev\"",
        "start": "\"$NODE\" --inspect dist/index.js",
        "lint": "eslint src --ext .js,.ts --ignore-path .gitignore",
        "format": "prettier --write **/*",
        "postinstall": "husky install"
    },
    "keywords": [
        "discord.js",
        "bot"
    ],
    "author": "Lexi Sother",
    "contributors": [
        {
            "email": "hades785@gmail.com",
            "name": "Loïc Bréard",
            "url": "http://loic-breard.ovh/"
        }
    ],
    "license": "Apache-2.0"
}
