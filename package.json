{
		"name": "nova",
		"version": "0.0.1",
		"description": "A Discord bot built on Discord.JS v12 with TypeScript",
		"main": "dist/index.js",
		"dependencies": {
				"dotenv": "^8.2.0",
				"ecstar": "^3.3.0"
		},
		"devDependencies": {
				"@types/node": "^14.11.2",
				"@types/ws": "^7.2.6",
				"ts-node": "^9.0.0",
				"tsc-watch": "^4.2.9",
				"typescript": "^4.0.2"
		},
		"scripts": {
				"build": "tsc",
				"start": "node dist/index.js",
				"once": "tsc && npm start",
				"dev": "tsc-watch --onSuccess \"node dist/index.js dev\"",
				"test": "mocha --require ts-node/register --extension ts --recursive --exit --unhandled-rejections=strict"
		},
		"keywords": [
				"discord.js",
				"bot"
		],
		"author": "Lexi Sother",
		"contributors": [
