{
    "name": "nova",
    "version": "0.0.1",
    "description": "A Discord bot built on Discord.JS v12 with TypeScript",
    "main": "dist/index.js",
    "dependencies": {
      "chalk": "^4.1.0",
      "discord.js": "^12.2.0",
      "inquirer": "^7.3.1",
      "moment": "^2.27.0"
    },
    "devDependencies": {
      "ts-node": "^9.0.0",
      "tsc-watch": "^4.2.9",
      "typescript": "^3.9.6"
    },
    "scripts": {
      "build": "tsc && npm prune --production",
      "start": "node dist/index.js",
      "once": "tsc && npm start",
      "dev": "tsc-watch --onSuccess \"node dist/index.js dev\""
    },
    "keywords": [
      "discord.js",
      "bot"
    ],
    "author": "Lexi Sother",
