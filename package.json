{
		"name": "nova",
		"version": "0.0.1",
		"description": "A Discord bot built on Discord.JS v12 with TypeScript",
		"main": "dist/index.js",
		"dependencies": {
				"discord.js": "^12.3.1"
		},
		"devDependencies": {
				"@types/inquirer": "^7.3.1",
				"@types/mocha": "^8.0.3",
				"@types/node": "^14.6.4",
				"@types/ws": "^7.2.6",
				"inquirer": "^7.3.3",
				"mocha": "^8.1.3",
				"ts-node": "^9.0.0",
				"tsc-watch": "^4.2.9",
				"typescript": "^4.0.2"
		},
		"scripts": {
				"build": "tsc",
				"start": "node dist/index.js",
				"once": "tsc && npm start",
				"dev": "tsc-watch --onSuccess \"node dist/index.js dev\"",
				"test": "mocha --require ts-node/register --extension ts --recursive --exit --unhandled-rejections=strict"
		},
		"keywords": [
				"discord.js",
				"bot"
		],
		"author": "Lexi Sother",
		"contributors": [
				{
						"email": "hades785@gmail.com",
						"name": "Loïc Bréard",
						"url": "http://loic-breard.ovh/"
				}
		],
		"license": "Apache-2.0"
}
